{
  "name": "dataloom-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:web": "next dev",
    "dev:electron": "ELECTRON_DEV=1 electron .",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "test": "vitest"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.3.4",
    "@codemirror/language": "^6.11.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@tmcw/togeojson": "^7.1.2",
    "@types/js-yaml": "^4.0.9",
    "@uiw/react-codemirror": "^4.24.2",
    "alasql": "^4.6.6",
    "apache-arrow": "^21.0.0",
    "chart.js": "^4.5.0",
    "chartjs-chart-matrix": "^3.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "diff": "5.2.0",
    "docx": "^9.5.1",
    "fflate": "^0.8.2",
    "github-markdown-css": "^5.8.1",
    "github-slugger": "^2.0.0",
    "isomorphic-git": "^1.33.2",
    "js-yaml": "^4.1.0",
    "json-formatter-js": "^2.5.23",
    "jstat": "^1.9.6",
    "marked": "^16.1.2",
    "mermaid": "^11.9.0",
    "moo": "^0.5.2",
    "nearley": "^2.20.1",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.54",
    "plotly.js": "^3.0.3",
    "plotly.js-dist": "^3.0.3",
    "plotly.js-dist-min": "^3.0.3",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-force-graph": "^1.48.0",
    "react-force-graph-2d": "^1.28.0",
    "react-icons": "^5.5.0",
    "react-json-view-lite": "^2.4.2",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "reactflow": "^11.11.4",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "shpjs": "^6.2.0",
    "turndown": "^7.2.0",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/marked": "^5.0.2",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/plotly.js": "^3.0.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/turndown": "^5.0.5",
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.6.0"
  },
  "main": "electron/main.js",
  "build": {
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "appId": "studio.dataloom",
    "productName": "DataLoom Studio",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "files": [
      "electron/**",
      ".next/**",
      "public/**",
      "package.json",
      "node_modules/**"
    ],
    "asar": true,
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "artifactName": "${productName}-${version}-${os}-${arch}-installer.${ext}"
    },
    "portable": {
      "artifactName": "${productName}-${version}-${os}-${arch}-portable.${ext}"
    }
  }
}
